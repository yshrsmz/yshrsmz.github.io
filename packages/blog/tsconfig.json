{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "verbatimModuleSyntax": true,
    "jsx": "preserve",
    "lib": ["esnext", "dom", "dom.iterable"],
    "typeRoots": [".vitepress/theme/types/"],
  },
  "include": ["./.vitepress/**/*"]
}
